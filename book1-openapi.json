{
    "swagger": "2.0",
    "info": {
      "title": "User API",
      "version": "1.0.0",
      "description": "An example API for managing users"
    },
    "basePath": "/api/v1",
    "paths": {
      "/users": {
        "get": {
          "summary": "Get a list of users",
          "responses": {
            "200": {
              "description": "Successful response",
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "example": 1
                    },
                    "name": {
                      "type": "string",
                      "example": "John Doe"
                    },
                    "email": {
                      "type": "string",
                      "format": "email",
                      "example": "john@example.com"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  